script(readToken, class, bike, sensor, end, start, threshold) ::= <<
'<readToken>'
'read_token' STORE

[
  $read_token
  '<class>'
  { 'bike_id' '<bike>' 'sensor_id' '<sensor>' }
  '<end>'
  '<start>'
] FETCH

[
  SWAP
  bucketizer.mean
  0 1 s 0
] BUCKETIZE

[
  SWAP
  <threshold> mapper.gt
  0 0 0
] MAP

[
  SWAP
  mapper.hdist
  MAXLONG
  MAXLONG
  1
] MAP
>>